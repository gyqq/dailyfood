        <text class="right address-phone">{{orderDetail.ship_tel}}</text>
      </view>
      <view class="address">
        <image class="address-icon left" style="margin-top: 15rpx" src="../../../image/order/icon-address.png"></image>
        <view class="left">
          <text class="address-lable" style="margin-left: 0">收货地址:</text>
          <text class="address-value">{{orderDetail.ship_area}}</text>
        </view>

      </view>
      <view class="address">
        <image class="address-icon left" src="../../../image/order/icon-invoice.png"></image>
        <text class="address-lable left">发票抬头:</text>
        <text class="address-value left">{{orderDetail.tax_type=="false"?'无':'有'}}</text>
      </view>
