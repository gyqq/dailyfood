    <view class="orderDetail-product">
      <view class="product-text">订单商品列表</view>
      <view class="confirmOrder-productIfo">
        <view class="pro-item">
          <image src="{{orderDetail.groups.groupon_image}}" class="pro-img left"></image>
          <view class="pro-ifo left">
            <text class="pro-title">{{orderDetail.groups.groupon_name}}</text>
            <view class="pro-price">
              <text class="pro-num">{{orderDetail.groups.person}}人拼团价</text>
              <text class="pro-groupPrice">￥{{orderDetail.groups.groupon_price}}</text>
              <text class="pro-originalPrice">￥{{orderDetail.groups.origin_price}}</text>
            </view>
          </view>
        </view>

      </view>
