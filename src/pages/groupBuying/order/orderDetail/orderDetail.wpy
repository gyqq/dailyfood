        <text class="left">{{orderDetail.createtimeStr}}</text>
      </view>
      <view class="ifo" wx:if="{{orderDetail.typeText=='待成团' || orderDetail.typeText=='待发货' ||orderDetail.typeText=='待收货' ||orderDetail.typeText=='已完成'}}">
        <text class="left">付款时间:</text>
        <text class="left"></text>
      </view>
      <view class="ifo" wx:if="{{orderDetail.typeText=='待收货' ||orderDetail.typeText=='已完成'}}">
        <text class="left">发货时间:</text>
        <text class="left"></text>
      </view>
      <view class="ifo" wx:if="{{orderDetail.typeText=='已完成'}}">
        <text class="left">收货时间:</text>
        <text class="left"></text>
      </view>
      <view class="ifo" wx:if="{{orderDetail.typeText=='已取消' || orderDetail.typeText=='(团购结束)已过期' ||orderDetail.typeText=='(未支付)已过期'}}">
        <text class="left">关闭时间:</text>
        <text class="left"></text>
      </view>
    </view>
    <view class="orderDetail-operation">
      <view wx:if="{{orderDetail.typeText=='待付款'}}">
        <text class="operation-btn right" @tap="toPay" style="color:#b58151;border: 1rpx solid #b58151">去付款</text>
        <text class="operation-btn right" @tap="cancelGroup">取消团购</text>
      </view>
      <view wx:if="{{orderDetail.typeText=='待成团'}}">
        <text class="operation-btn right" style="color:#b58151;border:1rpx solid #b58151">邀请好友</text>
        <text class="operation-btn right" @tap="groupState">拼团详情</text>
      </view>
      <view wx:if="{{orderDetail.typeText=='待发货'}}">
        <text class="operation-btn right" @tap="groupState">拼团详情</text>
      </view>
      <view wx:if="{{orderDetail.typeText=='待收货'}}">
        <text class="operation-btn right">查看物流</text>
        <text class="operation-btn right" @tap="groupState">拼团详情</text>
      </view>
      <view wx:if="{{orderDetail.typeText=='已完成'}}">
        <text class="operation-btn right" @tap="groupState">拼团详情</text>
      </view>
      <view wx:if="{{orderDetail.typeText=='已取消' ||orderDetail.typeText=='(团购结束)已过期' || orderDetail.typeText=='(未支付)已过期'}}">
        <text class="operation-btn right" @tap="deleteGroup">删除订单</text>
      </view>
    </view>
  </view>
</template>
<script>
  import wepy from 'wepy';
  import { connect } from 'wepy-redux';
  import util from '@/mixins/util'
  export default class orderDetail extends wepy.page {
    config = {
      navigationBarTitleText: '订单详情'
    };
    components = {};

   // mixins = [testMixin];

    data = {
      globalData:wepy.$instance.globalData,
      bagImgId:'',//背景图id
      bagTitle:'',//标题
      bagText:'',//描述
      order_id:'',//订单id
      orderType:'',//group  kill
      orderState:'',//订单状态:团购状态: 待付款,待成团,待发货,待收货,
      orderDetail:{},//订单详情
    };

    computed = {};

    methods = {
      callService(){//联系客服
        wx.navigateTo({
          url: './../../webView/webView?url=' + 'https://shinhocloud.kf5.com/kchat/?lang=zh_CN&active_in_iframe=1#/chatbox'
        })
      },
      copyBtn(){//复制
        wepy.setClipboardData({
          data: this.orderDetail.out_trade_no,
          success: function(res) {

          }
        })
      },
      cancelGroup(){//取消团购
        wepy.showLoading({
          title: '取消中',
        })
        var self =this
        var publicStr = wepy.$instance.getPublicData()
        let url = self.globalData.base_url+self.globalData.version_no+'/group-buy/cancel'+publicStr
        var data = {
          order_id:this.order_id,
          reason_type:7,
          _method:'DELETE',
          reason_desc:'不想要了'
        }
        wepy.request({
          url:url,
          method: "DELETE",
          data:data,
          header: {
            "Content-Type": "application/json; charset=UTF-8",  //post
          },
          success: function(res) {
            console.log(res)
            var res = res.data
            wepy.hideLoading()
            if (res.status=='1') {
              wepy.showToast({
                title: '订单已取消',
                icon: 'success',
                duration: 2000
              })
              wepy.navigateBack({
                delta: 1
              })
              // wepy.redirectTo({
              //   url: '../../../user/myGroup/myGroup'
              // })
            }else {
              wepy.showModal({
                title: '取消提示',
                content:res.status_txt,
                success: function(res) {
                }
              })
            }
            self.$apply()
          }
        })
      },
      toPay(){//去支付
        this.getPayData(this.orderDetail)
      },
      groupState(){//拼团详情
        wepy.navigateTo({
          url: '../../groupState/groupState?groupOn_id='+this.orderDetail.groupOn_id
        })
      },
      deleteGroup(){//删除订单
        wepy.showLoading({
          title: '取消中',
        })
        var self =this
        var publicStr = wepy.$instance.getPublicData()
        let url = self.globalData.base_url+self.globalData.version_no+'/orders/display'+publicStr
        var data = {
          order_id:this.order_id,
          _method:'PUT',
          status:false
        }
        wepy.request({
          url:url,
          method: "PUT",
          data:data,
          header: {
            "Content-Type": "application/json; charset=UTF-8",  //post
          },
          success: function(res) {
            console.log(res)
            var res = res.data
            wepy.hideLoading()
            if (res.status=='1') {
              wepy.showToast({
                title: '订单已删除',
                icon: 'success',
                duration: 2000
              })
              wepy.navigateBack({
                delta: 1
              })
            }else {
              wepy.showModal({
                title: '取消提示',
                content:res.status_txt,
                success: function(res) {
                }
              })
            }

            self.$apply()
          }
        })
      },
    };

    events = {};
    getOrderDetail(){
      //delivering //待收货
      // finish//已完成
      // cancel//已取消
      // dead //已作废
      // "active";//组团成功
      // "pendding";//进行中
      wepy.showLoading({
        title: '加载中',
      })
      var self =this
      var publicStr = wepy.$instance.getPublicData()
      let url = self.globalData.base_url+self.globalData.version_no+'/group-buy/order/'+this.order_id+publicStr
      console.log(url)
      wepy.request({
        url:url,
        method: "get",
        header: {
          "Content-Type": "application/json; charset=UTF-8",  //post
        },
        success: function(res) {
          console.log(res)
          var data = res.data.results[0]
          //收货地址整理
          data.ship_area = data.ship_area.split('mainland:')[1].split(':')+data.ship_addr
          //图片
          if (data.groups) {
            data.groups.groupon_image = self.globalData.base_url+self.globalData.version_no+'/img/'+data.groups.groupon_image
          }
          //创建时间
          data.createtimeStr = util.getMyDate(parseInt(data.createtime))
          //返回积分
          data.score_g= data.score_g.split('.')[0]
          //实付款
          data.final_amount = util.toDecimal2(data.final_amount)
          //运费
          data.total_amount = util.toDecimal2(data.total_amount)
          //商品合计
          data.cost_freight = util.toDecimal2(data.cost_freight)
          //团购状态

          if (data.status=='finish' || data.status=='uncommented') {//已完成
            data.typeText = '已完成'
          }
          if (data.status=='cancel' ||data.status=='dead') {//已取消
            data.typeText = '已取消'
          }
          //当status为active时用来区分待发货,待收货，然后为pedding时判断待付款,待成团、已过期
          //小分类  进行中:(待付款,待成团,待发货,待收货)
          if (data.status=='pendding' || data.status=='active') {//进行中
            //组团成功
            if (data.status=='active') {//分为 待收货  待发货
              if (data.ship_status=='0') {//待发货
                data.typeText = '待发货'
              }else if (data.ship_status=='1') {//待收货
                data.typeText = '待收货'
              }
            }else {//组团不成功  (未支付,已过期,待成团)  pendding
              var  create_time= parseInt(data.createtime)////data.groups.create_time
              var  remain_time = parseInt(data.groups.remain_time)
              var currentInterval = parseInt(Date.parse(new Date()))/1000;//获取当前时间戳
              var all = create_time+remain_time

              if (create_time+remain_time> currentInterval) {//组团未过期
                if (data.pay_status=='1') {//组团中
                  data.typeText = '待成团'
                }else {//未支付
                  //固定制服时间为120分钟  超出后不能支付
                  if (create_time+7200<currentInterval) {//超出支付时间
                    data.typeText = '(未支付)已过期'
                  }else {
                    data.typeText = '待付款'
                  }
                }
              }else {//组团已过期
                data.typeText = '(团购结束)已过期'
              }
            }
          }
          //data.typeText='(未支付)已过期' //测试 变更团购状态
          switch (data.typeText){
            case '已完成'://
              console.log('已完成')
              self.bagImgId = 'bg_groupOrder'
              self.bagTitle = '团购成功'//标题
              self.bagText = '您的每一次肯定都将成为我们前进的动力'//描述
              break;
            case '已取消'://
              console.log('已取消')
              self.bagImgId = 'bg_failOrder'
              self.bagTitle = '交易关闭'//标题
              self.bagText = '很遗憾此团购订单已被取消'//描述
              data.color = '#3f2021'
              break;
            case '待发货'://
              console.log('待发货')
              self.bagImgId = 'bg_groupOrder'
              self.bagTitle = '拼团已经成功'//标题
              self.bagText = '好东西不怕晚,请耐心等待我们的好食'//描述
              break;
            case '待收货'://
              console.log('待收货')
              self.bagImgId = 'bg_groupOrder'
              self.bagTitle = '好食已经出发'//标题
              self.bagText = '还剩19天23时自动确认收货'//描述
              break;
            case '待成团'://
              console.log('待成团')
              self.bagImgId = 'bg_groupOrder'
              self.bagTitle = '拼团还未成功'//标题
              self.bagText = '剩23小时59分自动取消团购订单,快邀请好友一起拼团'//描述
              break;
            case '(未支付)已过期'://
              console.log('(未支付)已过期')
              self.bagImgId = 'bg_failOrder'
              self.bagTitle = '交易关闭'//标题
              self.bagText = '很遗憾此团购订单已被取消'//描述
              data.color = '#3f2021'
              break;
            case '待付款'://
              console.log('待付款')
              self.bagImgId = 'bg_groupOrder'
              self.bagTitle = '等待买家付款'//标题
              self.bagText = '剩23小时59分自动取消团购订单'//描述
              break;
            case '(团购结束)已过期'://
              console.log('(团购结束)已过期')
              self.bagImgId = 'bg_failOrder'
              self.bagTitle = '交易关闭'//标题
              self.bagText = '很遗憾此团购订单已被取消'//描述
              data.color = '#3f2021'
              break;

            default:
              break;
          }

          self.orderDetail = data
         // console.log(self.orderDetail.typeText)
          console.log(self.orderDetail)
          wepy.hideLoading()
          self.$apply()
        }
      })
    }
    //支付开始========
    getPayData(item){//获取支付信息
      var self =this
      var publicStr = wepy.$instance.getPublicData()
      var openid = wepy.getStorageSync('openid')
      let url = self.globalData.base_url+self.globalData.version_no+'/small/weixinpay?openid='+openid+'&order_id='+item.order_id
      console.log(url)
      wepy.request({
        url:url,
        method: "get",
        header: {
          "Content-Type": "application/json; charset=UTF-8"  //post
        },
        success: function(res) {
          console.log('支付参数返回===========')
          console.log(res.data.results[0])
          wepy.hideLoading()
          self.pay(res.data.results[0])

        }
      })
    }
    pay(payData){
      var self =this
      wepy.requestPayment(
        {
          'timeStamp': payData.timeStamp,
          'nonceStr': payData.nonceStr,
          'package':  payData.package,
          'signType': 'MD5',
          'paySign': payData.paySign,
          'success': function (res) {
            console.log(res)
          },
          'fail': function (res) {
            console.log(res)
            self.getOrderDetail()
          },
          'complete': function (res) {//上线注释
            console.log('支付All')
            console.log(res)

          }
        })
    }
    onLoad(options) {
      this.order_id = options.order_id
      console.log('order_id='+options)
      this.getOrderDetail()
    }
  }
</script>
