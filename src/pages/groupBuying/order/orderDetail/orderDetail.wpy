        <!--{{groupIfo.is_free_delivery=='true'?'0':'NAN'}}-->
        <text class="right">￥{{orderDetail.total_amount}}</text>
      </view>
      <view class="confirmOrder-item">
        <text class="left">实付款(含运费)</text>
        <text class="right copeWith">￥{{orderDetail.final_amount}}</text>
      </view>
